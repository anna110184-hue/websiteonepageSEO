AI_REQUEST_TIMEOUT=30000

# Optional: Rate limiting (requests per minute)
RATE_LIMIT_MAX=100

# Security Configuration (for production)
# CORS_ORIGIN=https://yourdomain.com
# TRUST_PROXY=true